<VirtualHost *:80>
    DocumentRoot /opt/proj/web
    <Directory /opt/proj/web>
        Require all granted
        AllowOverride All
        Order Allow,Deny
        Allow from All
        Options FollowSymlinks
    </Directory>

    ErrorLog /var/log/apache2/proj_error.log
    CustomLog /var/log/apache2/proj_access.log combined
</VirtualHost>